{% if page_user.getUserAvatar is not null %}
    <p>{% media page_user.getUserAvatar.id, 'big' with {'alt': page_user.getUsername, 'title': '' } %}</p>
{% endif %}
{% if page_user.getUserHeroImage is not null %}
    <p>{% media page_user.getUserHeroImage.id, 'big' with {'alt': page_user.getUsername, 'title': '' } %}</p>
{% endif %}
<p>{{ page_user.getFullName }} owner</p>
